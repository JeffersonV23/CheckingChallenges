            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login.DatePickerFragment" />
        <activity android:name=".login.welcomeActivity.WelcomeActivity"/>
        <activity android:name=".login.MainActivity" />
        <activity android:name=".login.register.Register"/>
